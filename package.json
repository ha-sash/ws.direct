{
  "name": "ws.direct",
  "version": "1.2.2",
  "description": "RPC via WebSocket, access to the objects on server. And implementation of the Ext.direct via WebSocket.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ha-sash/ws.direct.git"
  },
  "keywords": [
    "websocket",
    "ext.direct",
    "rpc",
    "server object access"
  ],
  "author": "Aleksander Moskvitin <moskvandr@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "restify-clients": "^3.1.0",
    "tslint": "^5.20.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "eventemitter3": "^4.0.7",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings",
      "**/*.d.ts",
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "all": true
  }
